<!DOCTYPE html>
<html>
<head>
  <title>MonsterStrikeForTwitter</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
</head>

<body>
<% if logged_in? %>
  <%= render 'shared/header' %>
<% else %>
  <%= render 'shared/not_logged_in_header' %>
<% end %>

<%= yield %>
</body>
</html>
